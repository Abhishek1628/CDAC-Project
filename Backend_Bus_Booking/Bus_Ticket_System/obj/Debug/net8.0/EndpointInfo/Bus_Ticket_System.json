{
  "openapi": "3.0.1",
  "info": {
    "title": "Bus_Ticket_System",
    "version": "1.0"
  },
  "paths": {
    "/api/Auth/register": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Booking": {
      "post": {
        "tags": [
          "Booking"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BookingDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/BookingDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/BookingDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Booking/user/{userId}": {
      "get": {
        "tags": [
          "Booking"
        ],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Booking/{bookingId}": {
      "delete": {
        "tags": [
          "Booking"
        ],
        "parameters": [
          {
            "name": "bookingId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Route": {
      "get": {
        "tags": [
          "Route"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "tags": [
          "Route"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Route"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Route"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Route"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BookingDto": {
        "required": [
          "passengerGender",
          "passengerName",
          "routeId",
          "seatNumbers",
          "totalFare",
          "userId"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "routeId": {
            "type": "integer",
            "format": "int32"
          },
          "travelDate": {
            "type": "string",
            "format": "date-time"
          },
          "passengerName": {
            "minLength": 1,
            "type": "string"
          },
          "passengerAge": {
            "maximum": 120,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "passengerGender": {
            "minLength": 1,
            "type": "string"
          },
          "passengerPhone": {
            "type": "string",
            "format": "tel",
            "nullable": true
          },
          "seatNumbers": {
            "minLength": 1,
            "type": "string"
          },
          "totalFare": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "LoginDto": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RegisterDto": {
        "required": [
          "email",
          "fullName",
          "password"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 6,
            "type": "string"
          },
          "fullName": {
            "minLength": 1,
            "type": "string"
          },
          "phone": {
            "type": "string",
            "format": "tel",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Route": {
        "required": [
          "arrivalTime",
          "availableSeats",
          "baseFare",
          "busOperator",
          "busType",
          "departureTime",
          "duration",
          "fromCity",
          "toCity",
          "totalSeats"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "fromCity": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "toCity": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "departureTime": {
            "minLength": 1,
            "type": "string"
          },
          "arrivalTime": {
            "minLength": 1,
            "type": "string"
          },
          "duration": {
            "minLength": 1,
            "type": "string"
          },
          "baseFare": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "totalSeats": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "availableSeats": {
            "maximum": 2147483647,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "busOperator": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "busType": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "amenities": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}